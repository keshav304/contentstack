var personifyPersonalisation, configPersonalisation = {
    pages: {
        content: {
            name: "Content Page",
            type: "content",
            getContentName: function() {
                return "/"
            },
            isPage: !0,
            track: !0
        }
    },
    slots: {
        default: {
            id: "targeted-content",
            pos_element: targetedContentIdSelector,
            trigger: function() {
                personifyPersonalisation.decision(personifyPersonalisation, this, getVariants(targetedContentIdSelector), render)
            }
        }
    },
    actions: {},
    getUserID: function() {
        return ""
    },
    removeElements: [],
    trackElements: [],
    api: api,
    debug: !0,
    screenBreak: 600
};
function loadScript(e, t) {
    var n = document.head
      , r = document.createElement("script");
    r.type = "text/javascript",
    r.src = e,
    r.onreadystatechange = t,
    r.onload = t,
    n.appendChild(r)
}
var postPersonifyLoadPersonalisation = function() {
    (personifyPersonalisation = new PersonifyXP(configPersonalisation)).init()
};
function getVariants(e) {
    let t = []
      , n = document.querySelectorAll(e);
    for (let e = 0; e < n.length; e++) {
        let r = n[e]
          , i = JSON.parse(r.querySelector("#criteria").innerHTML)
          , o = getVariantCriteria(i.behaviours)
          , a = getVariantCriteria(i.tags);
        t.push({
            id: e,
            behaviours: o,
            tags: a,
            element: r
        })
    }
    return t
}
function getVariantCriteria(e) {
    try {
        return e.join("|")
    } catch (e) {
        return ""
    }
}
function render(e) {
    let t = getVariants(targetedContentIdSelector);
    for (let n = 0; n < t.length; n++) {
        let r = t[n];
        r.id == e ? r.element.hidden = !1 : r.element.hidden = !0
    }
}
"undefined" == typeof PersonifyXP ? loadScript("https://www.personifyxpassets.com/personify.library.0.23.js", postPersonifyLoadPersonalisation) : postPersonifyLoadPersonalisation();